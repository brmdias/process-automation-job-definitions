[ {
  "defaultTab" : "nodes",
  "description" : "Managed by Runbook Automation.",
  "executionEnabled" : true,
  "group" : "PagerDuty",
  "id" : "1394cbb9-9535-4143-8f5b-97ca0abc1eca",
  "loglevel" : "INFO",
  "name" : "API endpoint diagnostics (RBA)",
  "nodeFilterEditable" : false,
  "options" : [ {
    "name" : "apiendpoint",
    "value" : "https://www.pagerduty.com"
  }, {
    "hidden" : true,
    "name" : "pd_incident_id",
    "value" : "-1"
  } ],
  "plugins" : {
    "ExecutionLifecycle" : {
      "Send Incident Output to Pagerduty" : { }
    }
  },
  "runnerSelector" : {
    "filter" : "BDIAS",
    "runnerFilterMode" : "TAGS",
    "runnerFilterType" : "TAG_FILTER_AND"
  },
  "scheduleEnabled" : true,
  "schedules" : [ ],
  "sequence" : {
    "commands" : [ {
      "configuration" : {
        "authentication" : "None",
        "checkResponseCode" : "true",
        "headers" : "{\"content-type\": \"application/json\"}",
        "method" : "GET",
        "printResponse" : "true",
        "printResponseCode" : "true",
        "printResponseToFile" : "false",
        "proxySettings" : "false",
        "remoteUrl" : "${option.apiendpoint}",
        "responseCode" : "200",
        "sslVerify" : "true",
        "timeout" : "30000"
      },
      "errorhandler" : {
        "keepgoingOnSuccess" : true,
        "script" : "curl --request 'POST' \\\n--url 'https://events.eu.pagerduty.com/v2/enqueue' \\\n--header 'Content-Type: application/json' \\\n--data '{\n  \"payload\": {\n      \"summary\": \"Error @data.request_code@ on @option.apiendpoint@\",\n      \"severity\": \"critical\",\n      \"source\": \"Runbook Automation\",\n      \"custom_details\": {\n        \"problem\": \"@data.request_code@\",\n        \"endpoint\":\"@option.apiendpoint@\",\n        \"execution_id\":\"@job.execid@\"\n      }\n  },\n  \"routing_key\": \"R03DWCPINXA35S0EE8R9I00ITQN9F2TV\",\n  \"dedup_key\": \"@job.id@_@option.apiendpoint@\",\n  \"event_action\": \"trigger\"\n}'"
      },
      "nodeStep" : false,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "invalidKeyPattern" : "\\s|\\$|\\{|\\}|\\\\",
            "logData" : "true",
            "name" : "request_code",
            "regex" : "Response Code: HTTP/1.1 (.*)",
            "replaceFilteredResult" : "false"
          },
          "type" : "key-value-data"
        } ]
      },
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "configuration" : {
        "incident_id" : "${option.pd_incident_id}",
        "note" : "Runbook executed by ${job.username}.\nExecutions logs available at: https://pdt-rdtam.runbook.pagerduty.cloud/project/bdias/execution/show/${job.execid}"
      },
      "nodeStep" : false,
      "type" : "pd-note-step"
    } ],
    "keepgoing" : false,
    "pluginConfig" : {
      "LogFilter" : [ {
        "config" : {
          "customOutput" : "This is the result of the API endpoint diagnostics.",
          "filterName" : "API endpoint diagnostics",
          "logOutputSettings" : "All",
          "regexPattern" : "(.*)",
          "stepStatus" : "Any"
        },
        "type" : "pagerduty-incident-output-capture"
      } ],
      "WorkflowStrategy" : {
        "ruleset" : {
          "rules" : "[*] run-in-sequence\n[2] if:option.pd_incident_id!=-1"
        }
      }
    },
    "strategy" : "ruleset"
  },
  "uuid" : "1394cbb9-9535-4143-8f5b-97ca0abc1eca"
} ]
