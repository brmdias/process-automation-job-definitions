[ {
  "defaultTab" : "nodes",
  "description" : "",
  "executionEnabled" : true,
  "group" : "PagerDuty",
  "id" : "bc0c726f-a617-4f15-8232-c7b9bfa04422",
  "loglevel" : "INFO",
  "name" : "API endpoint monitoring",
  "nodeFilterEditable" : false,
  "notification" : {
    "onfailure" : {
      "plugin" : {
        "configuration" : {
          "authentication" : "None",
          "body" : "{\n  \"payload\": {\n      \"summary\": \"[${job.status}] \\\"${job.name}\\\" run by ${job.user} (#${job.execid})\",\n      \"severity\": \"critical\",\n      \"source\": \"Runbook Automation\",\n      \"component\": \"${job.id}\",\n      \"group\": \"${job.serverUrl}\",\n      \"custom_details\": {\n        \"execution_id\": \"${execution.href}\",\n        \"endpoint\": \"${option.apiendpoint}\",\n        }\n  },\n  \"routing_key\": \"R03DY3A0ISMLEPIECREOJCS2CMSBJFS0\",\n  \"event_action\": \"trigger\"\n}",
          "contentType" : "application/json",
          "file" : "/etc/rundeck/notification_responses",
          "headers" : "Accept: */*",
          "method" : "POST",
          "printResponseToFile" : "true",
          "remoteUrl" : "https://events.eu.pagerduty.com/v2/enqueue",
          "timeout" : "30000"
        },
        "type" : "HttpNotification"
      }
    },
    "onsuccess" : {
      "plugin" : {
        "configuration" : {
          "action" : "resolve",
          "component" : "${job.id}",
          "dedupe_key" : "${job.id}",
          "group" : "${job.serverUrl}",
          "images" : "https://www.rundeck.com/hubfs/rundeck-app-assets/rundeck-by-pagerduty-icon.png",
          "pagerdutyUrl" : "https://events.eu.pagerduty.com",
          "payload_severity" : "warning",
          "payload_source" : "Runbook Automation",
          "payload_summary" : "[${job.status}] \"${job.name}\" run by ${job.user} (#${job.execid})",
          "service_key_storage_path" : "keys/pagerduty/pdt-bdias-global-orchestration-key"
        },
        "type" : "PagerDutyEventNotification"
      }
    }
  },
  "notifyAvgDurationThreshold" : null,
  "options" : [ {
    "name" : "apiendpoint",
    "value" : "https://www.bbc.com"
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "runnerSelector" : {
    "runnerFilterMode" : "LOCAL",
    "runnerFilterType" : "LOCAL_RUNNER"
  },
  "scheduleEnabled" : true,
  "schedules" : [ ],
  "sequence" : {
    "commands" : [ {
      "configuration" : {
        "authentication" : "None",
        "checkResponseCode" : "false",
        "headers" : "{\"content-type\": \"application/json\"}",
        "method" : "GET",
        "printResponse" : "false",
        "printResponseCode" : "true",
