    }, {
      "configuration" : {
        "debugOnly" : "false"
      },
      "nodeStep" : false,
      "type" : "log-data-step"
    } ],
    "keepgoing" : true,
    "strategy" : "node-first"
  },
  "uuid" : "bc0c726f-a617-4f15-8232-c7b9bfa04422"
